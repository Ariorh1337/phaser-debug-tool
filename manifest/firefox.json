{
    "name": "Phaser Debugger",
    "description": "Debug tool for Phaser 3",
    "version": "",
    "manifest_version": 2,
    "content_scripts": [
        {
            "js": ["inject.js"],
            "matches": ["*://*/*"],
            "match_about_blank": true,
            "all_frames": true,
            "run_at": "document_start"
        }
    ],
    "icons": {
        "128": "icon.png",
        "16": "icon.png",
        "48": "icon.png"
    },
    "web_accessible_resources": ["bundle.js"]
}
